{
  "name": "risu-ebooklike-viwer",
  "version": "0.0.2",
  "type": "module",
  "description": "EBook like viewer for RISU AI",
  "main": "src/index.js",
  "browser": "dist/risu-ebooklike-viwer.js",
  "unpkg": "dist/risu-ebooklike-viwer.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "risu-ai",
    "cdn",
    "plugin",
    "svelte"
  ],
  "author": "Chaebboong",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:server\" \"npm run dev:vite\"",
    "dev:server": "node scripts/dev-server.js",
    "dev:vite": "cross-env NODE_ENV=development vite build --watch",
    "build": "vite build",
    "build:dev": "cross-env NODE_ENV=development vite build",
    "lint": "eslint src --ext .js,.svelte",
    "lint:fix": "eslint src --ext .js,.svelte --fix",
    "format": "prettier --write \"src/**/*.{js,svelte,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,svelte,json,css}\"",
    "release": "node scripts/release.js",
    "release:minor": "node scripts/release.js -- minor",
    "release:patch": "node scripts/release.js -- patch",
    "release:major": "node scripts/release.js -- major"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "prettier-plugin-svelte": "^3.3.2",
    "svelte": "^5.16.0",
    "svelte-eslint-parser": "^0.43.0",
    "terser": "^5.36.0",
    "vite": "^6.0.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "ws": "^8.18.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.4.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "idb": "^8.0.0",
    "flatpickr": "^4.6.13",
    "lodash": "^4.17.21",
    "lucide-svelte": "^0.468.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
